  function-arguments-create-frame:
    - match: \(
      scope: punctuation.section.group.begin.lua
      set:
        - method-arguments-end
        - frame-template
        - method-arguments-list
        - method-arguments-list
        - frame-type
    - include: else-pop

  frame-template:
    - match: (([''"])({{templates_frame}})([''"]))
      captures:
        1: variable.parameter.wow.template.frame
    - match: (([''"])(\w*)([''"]))
      captures:
        1: string.template.frame
    - include: else-pop

  frame-type:
    - match: (([''"])(?i)({{frame_types}})([''"]))
      captures:
        1: variable.parameter.wow.frametype
      set: method-arguments-list
    - match: (([''"])(\w*)([''"]))
      captures:
        1: string.frametype
      set: method-arguments-list
    - include: else-pop

  function-arguments-console-variable:
    - match: \(
      scope: punctuation.section.group.begin.lua
      set:
        - method-arguments-end
        - cvar-type
    - include: else-pop

  cvar-type:
    - match: (([''"])({{console_variables}})([''"]))
      captures:
        1: variable.parameter.wow.console.variable
    - match: (([''"])(\w*)([''"]))
      captures:
        1: string.cvartype
    - include: else-pop

  function-arguments-inventory-slot-info:
    - match: \(
      scope: punctuation.section.group.begin.lua
      set:
        - method-arguments-end
        - slot-type
    - include: else-pop

  slot-type:
    - match: (([''"])({{slot_types}})([''"]))
      captures:
        1: variable.parameter.wow.slottype
    - match: (([''"])(\w*)([''"]))
      captures:
        1: string.slottype
    - include: else-pop

  function-arguments-secure-handler-wrap-script:
    - match: \(
      scope: punctuation.section.group.begin.lua
      set:
        - method-arguments-end
        - script-type
        - method-arguments-list
    - include: else-pop

