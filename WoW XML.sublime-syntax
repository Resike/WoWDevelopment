    - match: string{{identifier_break}}
      scope: support.constant.builtin.lua
      set:
        - match: \.
          scope: punctuation.accessor.lua
          set:
            - match: |-
                (?x:
                  byte|char|(?# dump|)find|format|gfind|gmatch|gsub|join|len|lower|match|(?# pack|packsize|)rep|reverse|rtgsub|split|sub|trim|(?# unpack|)upper
                ){{identifier_break}}
              scope: meta.property.lua support.function.builtin.lua
              pop: 1
            - include: property
        - include: else-pop

    - match: table{{identifier_break}}
      scope: support.constant.builtin.lua
      set:
        - match: \.
          scope: punctuation.accessor.lua
          set:
            - match: |-
                (?x:
                  concat|foreach|foreachi|getn|insert|maxn|(?# move|pack|)remove|removemulti|setn|sort|wipe(?# |unpack)
                ){{identifier_break}}
              scope: meta.property.lua support.function.builtin.lua
              pop: 1
            - include: property
        - include: else-pop

