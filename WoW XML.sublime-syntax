  support:
    - match: '\b(?:__(?:add|call|concat|div|eq|index|len|le|lt|mod|mode|mul|newindex|pow|sub|unm))\b'
      scope: support.function.metamethod.lua
    - match: '\b(?:(?# basic|)assert|collectgarbage|(?# dofile|)error|getmetatable|ipairs|(?# load|loadfile|)next|pairs|pcall|print|rawequal|rawget|(?# rawlen|)rawset|(?# require|)select|setmetatable|tonumber|tostring|type|xpcall)\b'
      scope: support.function.lua
    - include: constants
    - include: bit
